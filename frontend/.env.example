# API Keys (add as needed)
# NEXT_PUBLIC_GOOGLE_CALENDAR_API_KEY=""
# NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=""
# STRIPE_SECRET_KEY=""

# Email Service (if needed)
# SMTP_HOST=""
# SMTP_PORT=""
# SMTP_USER=""
# SMTP_PASSWORD=""
# EMAIL_FROM=""

# Storage (if using cloud storage)
# AWS_ACCESS_KEY_ID=""
# AWS_SECRET_ACCESS_KEY=""
# AWS_REGION=""
# AWS_S3_BUCKET=""

# Analytics
# NEXT_PUBLIC_GA_MEASUREMENT_ID=""

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
